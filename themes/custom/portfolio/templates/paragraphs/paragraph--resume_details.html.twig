<div class="resume-item ">
    <h4>
        {{ content.field_title.0|raw }}
    </h4>
    {% if content.field_year[0] %}
        <h5>
            {{ content.field_year.0 }}
        </h5>
    {% endif %}
    <p>
        <em>
            {{ content.field_descriptions.0 }}
        </em>
    </p>
    {% for key, value in content.field_paragraph['#items'] %}
    <p>
        {{ content.field_paragraph[key] }}
    </p>
{% endfor %}
    <ul>
        {% for key, value in content.field_notes['#items'] %}

            <li>
                {{ content.field_notes[key] }}
            </li>
        {% endfor %}
    </ul>
</div>
