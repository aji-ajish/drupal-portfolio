  <!-- ======= Services Section ======= -->
  <section id="services" class="services">
  <div class="container">

    <div class="section-title">
    <h2> {{ content.field_title.0 }}</h2>
    <p> {{ content.field_description.0 }}</p>    
</div>

    <div class="row">
    {% for key, value in content.field_services_details['#items'] %}
      <div class="col-lg-4 col-md-6 icon-box" data-aos="fade-up {{ content.field_services_details[key]['#paragraph'].field_delay_time.value }}">
        <div class="icon"><i class="bi {{ content.field_services_details[key]['#paragraph'].field_icon.value }}"></i></div>
        <h4 class="title"><a href="">{{ content.field_services_details[key]['#paragraph'].field_title.value }}</a></h4>
        <p class="description">{{content.field_services_details[key]['#paragraph'].field_description.value }}</p>
      </div>
    {% endfor %}
    </div>

  </div>
</section><!-- End Services Section -->
